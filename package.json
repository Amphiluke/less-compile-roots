{
  "name": "less-compile-roots",
  "version": "2.0.1",
  "description": "Tool for extracting and compiling root Less files",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./src/index.mjs",
  "exports": {
    "import": "./src/index.mjs",
    "require": "./dist/index.cjs"
  },
  "bin": {
    "less-compile-roots": "./bin/less-compile-roots.cjs"
  },
  "files": [
    "bin",
    "dist",
    "src"
  ],
  "scripts": {
    "lint": "eslint",
    "build": "rollup -c"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Amphiluke/less-compile-roots.git"
  },
  "keywords": [
    "less",
    "glob"
  ],
  "author": "Amphiluke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Amphiluke/less-compile-roots/issues"
  },
  "homepage": "https://github.com/Amphiluke/less-compile-roots#readme",
  "dependencies": {
    "fast-glob": "^3.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.11.1",
    "@stylistic/eslint-plugin-js": "^2.8.0",
    "eslint": "^9.11.1",
    "globals": "^15.9.0",
    "rollup": "^4.22.4"
  },
  "peerDependencies": {
    "less": ">=2.0.0"
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
